generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Cliente {
  id         Int    @id @default(autoincrement())
  nome       String
  email      String @unique
  telefone   String
  descricao  String
  documento  String?
}

model Usuario {
  id    Int    @id @default(autoincrement())
  email String @unique
  senha String 
}